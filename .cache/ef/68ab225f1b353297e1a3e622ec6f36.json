xtends LitElement {\n  constructor(){\n    super()    \n  }\n\n  static get properties(){\n    return {\n        id: {\n          type: String\n        },\n        name: {\n            type: String\n        },\n        day: {\n            type: String\n        },\n        time: {\n            type: String\n        },\n        user: {\n            type: Object\n        },  \n        image: {\n            type: String\n        }, \n        bio: {\n          type: String\n      }, \n    }\n  }\n\n  firstUpdated(){\n    super.firstUpdated()\n  }\n\n  bioHandler(){\n    // create sl-dialog \n    const dialogEl = document.createElement('sl-dialog')\n    // add class name\n    dialogEl.className = 'doctor-dialog'\n    // sl-dialog content\n    const dialogContent = html`\n    <style>\n      .wrap {\n        display: flex;\n      }\n      .image {\n        width: 50%;\n      }\n      .image img {\n        width: 100%;\n      }\n      .content {\n        padding-left: 1em;\n      } \n\n      .sl-button {\n        color: var(--brand-color);\n      }\n    </style>\n\n    <div class=\"wrap\">\n      <div class=\"image\">\n        <img src=\"${App.apiBase}/images/${this.image}\" alt=\"${this.name}\" />\n      </div>\n      <div class=\"content\">\n        <h1>${this.name}</h1>\n        <p>${this.bio}</p>\n     \n  \n\n        <sl-button @click=${this.addAppointmentHandler.bind(this)}>\n         \n        </sl-button>\n      </div>\n    </div>\n    `\n\n    render(dialogContent, dialogEl)\n\n    // append to document.body\n    document.body.append(dialogEl)\n\n    // show sl-dialog\n    dialogEl.show()\n\n    // on hide delete dialogEl\n    dialogEl.addEventListener('sl-after-hide', () => {\n      dialogEl.remove()\n    })\n  }\n\n  async addAppointmentHandler(){    \n    try {\n      await UserAPI.addAppointment(this.id)\n      Toast.show('Successfully Booked')\n    }catch(err){\n      Toast.show(err, 'error')\n    }\n    AppointmentAPI.deleteAppointment(this.id)\n  }\n\n  async cancelAppointmentHandler(){\n    try {\n      await UserAPI.cancelAppointment(this.id)\n      Toast.show('Successfully Canceled')\n    }catch(err){\n      Toast.show(err, 'error')\n    }\n    location.reload(true);\n  }\n  \n  render(){    \n    return html`\n    \n    <style>\n    .doctor {\n      text-align: center;\n    }\n\n    img {\n      width: 20em;\n    }\n\n    h2 {\n     text-align: center;\n    }\n\n    .sl-button {\n      color: var(--brand-color);\n      left: 50%;\n    }\n    </style>\n\n    <sl-card class= \"doctor\"> \n      <img slot=\"image\" src=\"${App.apiBase}/images/${this.image}\" />\n      <h2>${this.name}</h2>\n      <p>${this.day}</p>\n      <p>${this.time}</p>\n     \n\n      <sl-button @click=${this.cancelAppointmentHandler.bind(this)}>Cancel</sl-button>\n    </sl-card>\n    \n  `}\n})"},"lineCount":null}},"error":null,"hash":"ff427db3aff012a24eca3f331db5399e","cacheData":{"env":{}}}